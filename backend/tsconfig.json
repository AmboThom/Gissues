{
    "compilerOptions": {
      "target": "ES2022",
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "outDir": "./dist",
      "strict": true,
      "noImplicitAny": false,
      "skipLibCheck": true,
      "types": ["node", "express"]
    },
    "include": ["src/**/*"]
  }